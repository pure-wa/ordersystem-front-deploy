{"ast":null,"code":"import { getResultData } from '@/utils/commonDataHandler';\nimport axios from 'axios';\nexport default {\n  // 화면 간 매개변수를 넘길 때 사용하는 변수값을 props라 칭함\n  props: ['isMyPage', 'pageTitle'],\n  // 분기처리의 대상이 되는 매개변수를 나열\n\n  data() {\n    return {\n      // memberDetail: null,\n      memberDetail: {}\n    };\n  },\n  async created() {\n    try {\n      // props 생성 후 분기 처리\n      // MyPage인 경우\n      if (this.isMyPage) {\n        // axios의 요청을 main.js에 에서 인터셉팅하여 토큰 확인 처리로 위는 주석 처리\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/member/myinfo`); // 서버 엔드포인트가 /member/myinfo\n        this.memberDetail = getResultData(response);\n      } else {\n        // MyPage 아닌 경우\n        const memberId = this.$route.params.id;\n        // axios의 요청을 main.js에 에서 인터셉팅하여 토큰 확인 처리로 위는 주석 처리\n        const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/member/detail/${memberId}`);\n        this.memberDetail = getResultData(response);\n      }\n    } catch (e) {\n      console.log(e);\n    }\n  }\n};","map":{"version":3,"names":["getResultData","axios","props","data","memberDetail","created","isMyPage","response","get","process","env","VUE_APP_API_BASE_URL","memberId","$route","params","id","e","console","log"],"sources":["/Users/purewa/Desktop/Pratice/Front_Pratice/vue-project/src/components/MemberDetailComponent.vue"],"sourcesContent":["<template>\n        <v-container>\n        <v-row>\n            <v-col>\n                <v-card>\n                    <v-card-title class=\"text-center text-h5\">\n                        {{ pageTitle }}\n                    </v-card-title>\n                    <v-card-text>\n                        <v-table>\n                        <!-- <v-table v-if=\"memberDetail\"> -->\n                            <tbody>\n                                <tr>\n                                    <td>ID : </td>\n                                    <td>{{ memberDetail.id }}</td>\n                                </tr>\n                                <tr >\n                                    <td>이름 : </td>\n                                    <td>{{ memberDetail.name }}</td>\n                                </tr>\n                                <tr>\n                                    <td>이메일 : </td>\n                                    <td>{{ memberDetail.email }}</td>\n                                </tr>\n                            </tbody>\n                        </v-table>\n                    </v-card-text>\n                </v-card>\n            </v-col>\n        </v-row>\n    </v-container>\n</template>\n\n<script>\nimport { getResultData } from '@/utils/commonDataHandler';\nimport axios from 'axios';\n\n\nexport default{\n    // 화면 간 매개변수를 넘길 때 사용하는 변수값을 props라 칭함\n    props: ['isMyPage', 'pageTitle'], // 분기처리의 대상이 되는 매개변수를 나열\n    \n    data(){\n        return{\n            // memberDetail: null,\n            memberDetail: {}\n        }\n    },\n\n    async created(){\n        try{\n            // props 생성 후 분기 처리\n            // MyPage인 경우\n            if (this.isMyPage){\n            // axios의 요청을 main.js에 에서 인터셉팅하여 토큰 확인 처리로 위는 주석 처리\n            const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/member/myinfo`);  // 서버 엔드포인트가 /member/myinfo\n            this.memberDetail = getResultData(response);\n            } else{      // MyPage 아닌 경우\n            const memberId = this.$route.params.id;\n            // axios의 요청을 main.js에 에서 인터셉팅하여 토큰 확인 처리로 위는 주석 처리\n            const response = await axios.get(`${process.env.VUE_APP_API_BASE_URL}/member/detail/${memberId}`);\n            this.memberDetail = getResultData(response);\n            }\n        } catch(e){\n            console.log(e);\n        }\n    },\n}\n</script>"],"mappings":"AAkCA,SAASA,aAAY,QAAS,2BAA2B;AACzD,OAAOC,KAAI,MAAO,OAAO;AAGzB,eAAc;EACV;EACAC,KAAK,EAAE,CAAC,UAAU,EAAE,WAAW,CAAC;EAAE;;EAElCC,IAAIA,CAAA,EAAE;IACF,OAAM;MACF;MACAC,YAAY,EAAE,CAAC;IACnB;EACJ,CAAC;EAED,MAA