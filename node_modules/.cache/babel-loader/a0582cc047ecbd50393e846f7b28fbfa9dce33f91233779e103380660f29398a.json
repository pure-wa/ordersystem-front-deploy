 <!-- 모달창(팝업) 위한 v-dialog  -->\n    <v-btn @click=\"cancle()\">클릭하기</v-btn>\n    <!-- v-dialog는 내부적으로 true/false값을 가지고 외부 공개할지 말지 여부가 결정 -->\n    <v-dialog v-model=\"trueOrFalse\" max-height=\"500px\">\n      <v-card>\n         <v-card-title>\n            정말 취소하시겠습니까?\n         </v-card-title>\n         <v-card-actions>\n            <v-btn>예</v-btn>\n            <v-btn>아니오</v-btn>\n         </v-card-actions>\n      </v-card>\n    </v-dialog>\n\n</template>\n\n<script>\nexport default {\n   data(){\n      return {\n         trueOrFalse: false\n      }\n   },\n   methods: {\n      cancle(){\n         this.trueOrFalse = !this.trueOrFalse  \n      }\n   }\n}\n\n</script>\n<style>\n#signUp-title{\n   color:red;\n}\n</style>"],"mappings":"AAoFA,eAAe;EACZA,IAAIA,CAAA,EAAE;IACH,OAAO;MACJC,WAAW,EAAE;IAChB;EACH,CAAC;EACDC,OAAO,EAAE;IACNC,MAAMA,CAAA,EAAE;MACL,IAAI,CAACF,WAAU,GAAI,CAAC,IAAI,CAACA,WAAU;IACtC;EACH;AACH","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}